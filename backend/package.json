{
  "name": "backend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -f .env -- tsx watch src/server.ts",
    "start": "tsx src/server.ts",
    "build": "npm run prisma:generate && npm run db:setup",
    "format": "prettier --write './**/*.{ts,mjs,json}'",
    "lint": "eslint .",
    "prisma:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:explore": "prisma studio",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "test": "jest"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.14.1",
    "@prisma/client": "^5.20.0",
    "body-parser": "^1.20.2",
    "bullmq": "^5.13.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.64.0",
    "eslint": "^9.9.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  },
  "devDependencies": {
    "@eslint/core": "^0.6.0",
    "@eslint/js": "^9.9.1",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.9.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  }
}
